{
  "name": "headless-browser",
  "version": "0.0.2",
  "description": "Crawl websites with PhantomJS or Puppeteer",
  "main": "lib/app.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node lib/app.js",
    "clean": "rimraf lib",
    "build": "webpack",
    "watch": "webpack --watch",
    "dev": "cross-env DEBUG=true ts-node-dev src/index.ts",
    "prod": "ts-node-dev src/index.ts",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": "https://github.com/kevupton/headless-browser",
  "author": "Kevin Upton",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-typescript": "7.3.3",
    "@types/argparse": "1.0.36",
    "@types/express": "4.16.1",
    "@types/lodash": "^4.14.123",
    "@types/node": "11.13.8",
    "@types/phantom": "3.2.5",
    "@types/phantomjs": "1.9.29",
    "@types/puppeteer": "1.12.3",
    "@types/source-map-support": "0.5.0",
    "babel-loader": "8.0.5",
    "babel-node": "7.0.0-beta.3",
    "babel-plugin-transform-class-properties": "7.0.0-beta.3",
    "babel-plugin-transform-runtime": "7.0.0-beta.3",
    "cross-env": "5.2.0",
    "rimraf": "2.6.3",
    "ts-loader": "5.4.5",
    "ts-node-dev": "1.0.0-pre.32",
    "typescript": "3.4.5",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "argparse": "1.0.10",
    "core-js": "3",
    "express": "4.16.4",
    "lodash": "4.17.11",
    "phantom": "6.2.0",
    "puppeteer": "1.15.0",
    "rxjs": "6.5.1",
    "source-map-support": "0.5.9"
  },
  "preferGlobal": true,
  "bin": {
    "headless-browser": "bin/headless-browser.js"
